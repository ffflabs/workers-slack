{
  "name": "@sagi.io/cfw-slack",
  "version": "0.0.11",
  "description": "Slack API for Cloudflare Workers",
  "main": "dist/main.js",
  "author": "Sagi Kedmi <git@sagi.io> (https://sagi.io)",
  "homepage": "https://sagi.io",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:babel": "babel --ignore '**/*.test.js' src -d lib --verbose",
    "build:webpack": "webpack --display-modules ./lib",
    "build": "yarn build:babel && yarn build:webpack",
    "bump:push": "yarn version --patch && git push origin master --follow-tags",
    "lint": "eslint ./src",
    "test": "jest",
    "prepush": "yarn build && yarn lint && yarn test",
    "prepublishOnly": "yarn build && yarn lint && yarn test && yarn bump:push"
  },
  "dependencies": {
    "@sagi.io/globalthis": "^0.0.2",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-prettier": "^3.1.0",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "coveralls": "^3.0.3",
    "cross-fetch": "^3.0.3",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "lodash.get": "^4.4.2",
    "prettier": "^1.17.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagi/cfw-slack.git"
  },
  "keywords": ["cloudflare", "workers", "cloudflare workers", "slack", "rest"]
}

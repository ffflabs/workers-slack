    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "cross-fetch": "^3.0.6",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lodash.get": "^4.4.2",
    "prettier": "^2.1.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"

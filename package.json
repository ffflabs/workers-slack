{
  "name": "@sagi.io/cfw-slack",
  "version": "0.0.23",
  "description": "Slack API for Cloudflare Workers",
  "main": "dist/main.js",
  "author": "Sagi Kedmi <git@sagi.io> (https://sagi.io)",
  "homepage": "https://sagi.io",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:babel": "babel --ignore '**/*.test.js' src -d lib --verbose",
    "build:webpack": "webpack --display-modules ./lib",
    "build": "yarn build:babel && yarn build:webpack",
    "bump:push": "yarn version --patch && git push origin master --follow-tags",
    "lint": "eslint ./src",
    "test": "jest",
    "prepublishOnly": "yarn build && yarn lint && yarn test && yarn bump:push"
  },
  "dependencies": {
    "@sagi.io/globalthis": "^0.0.2",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.0.9",
    "cross-fetch": "^3.0.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lodash.get": "^4.4.2",
    "prettier": "^1.19.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagi/cfw-slack.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare workers",
    "slack",
    "rest"
  ]
}
